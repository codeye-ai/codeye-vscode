{
  "name": "codeye",
  "displayName": "Codeye",
  "description": "Visual Studio Code extension and CLI tool to read, fix, and write code as good as or even better than a human software engineer.",
  "icon": "icon.png",
  "version": "0.1.3",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "AI",
    "Chat",
    "Machine Learning",
    "Snippets"
  ],
  "publisher": "codeye",
  "activationEvents": [],
  "main": "extension.build.js",
  "bin": {
    "codeye": "cli.build.js"
  },
  "contributes": {
    "commands": [
      {
        "command": "codeye.startSession",
        "title": "Codeye: Start session"
      },
      {
        "command": "codeye.startSessionAfresh",
        "title": "Codeye: Start fresh session"
      }
    ],
    "configuration": {
      "title": "Codeye",
      "properties": {
        "codeye.geminiApiKey": {
          "type": "string",
          "description": "Your Google Gemini or AI Studio API key."
        },
        "codeye.openAiApiKey": {
          "type": "string",
          "description": "Your OpenAI API key."
        },
        "codeye.openAiOrganization": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "Your OpenAI organization ID (if any)."
        },
        "codeye.aiModel": {
          "type": [
            "string"
          ],
          "enum": [
            "gemini-1.5-flash-latest",
            "gemini-1.5-pro-latest",
            "gpt-4o",
            "gpt-3.5-turbo"
          ],
          "enumDescriptions": [
            "Designed for high-volume, high-frequency tasks where cost and latency matter.",
            "Designed for larger context and robust multimodal i.e., text, image etc. as input.",
            "Use the (latest) GPT-4o model and works well with projects of all sizes with text and image files.",
            "Use the GPT-3.5 Turbo model which is cheaper but only suitable from small projects."
          ],
          "enumItemLabels": [
            "Gemini 1.5 Flash",
            "Gemini 1.5 Pro",
            "GPT-4o",
            "GPT-3.5 Turbo"
          ],
          "default": "gpt-4o",
          "description": "Model to use for code generation. Could be one of a Google Gemini or OpenAI ChatGPT model identifier."
        }
      }
    },
    "menus": {
      "commandPalette": [
        {
          "command": "codeye.startSession"
        },
        {
          "command": "codeye.startSessionAfresh"
        }
      ]
    },
    "terminal": {
      "profiles": [
        {
          "id": "codeye.terminalProfile",
          "icon": "terminal",
          "title": "Codeye"
        }
      ]
    }
  },
  "scripts": {
    "debug": "node cli.js -rv",
    "esbuild-cli": "esbuild ./cli.js --bundle --outfile=cli.build.js --format=cjs --platform=node",
    "esbuild-extension": "esbuild ./extension.js --bundle --outfile=extension.build.js --external:vscode --format=cjs --platform=node",
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "vscode-test",
    "vscode:prepublish": "npm run esbuild-cli -- --minify && npm run esbuild-extension -- --minify"
  },
  "dependencies": {
    "@google/generative-ai": "^0.12.0",
    "chalk": "4.1.2",
    "crc-32": "^1.2.2",
    "dotenv": "^16.4.5",
    "getos": "^3.2.1",
    "mime-types": "^2.1.35",
    "openai": "^4.47.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.80.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "@vscode/vsce": "^2.27.0",
    "esbuild": "^0.21.4",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  }
}
