{
  "version": "0.2.0",
  "configurations": [
    {
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "name": "CLI",
      "request": "launch",
      "runtimeArgs": ["run", "debug"],
      "runtimeExecutable": "npm",
      "type": "node"
    },
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"]
    }
  ]
}
