{
  "name": "codeye",
  "displayName": "Codeye",
  "description": "CLI tool to read, fix, and write code as good as or even better than a human software engineer.",
  "icon": "icon.png",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "AI",
    "Chat",
    "Machine Learning",
    "Snippets"
  ],
  "publisher": "codeye",
  "activationEvents": [],
  "main": "./extension.js",
  "bin": {
    "codeye": "cli.js"
  },
  "contributes": {
    "commands": [
      {
        "command": "codeye.startSession",
        "title": "Codeye: Start session"
      },
      {
        "command": "codeye.startSessionAfresh",
        "title": "Codeye: Start fresh session"
      }
    ],
    "configuration": {
      "title": "Codeye",
      "properties": {
        "codeye.openAiApiKey": {
          "type": "string",
          "description": "Your OpenAI API key."
        },
        "codeye.openAiOrganization": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "Your OpenAI organization ID (if any)."
        },
        "codeye.openAiModel": {
          "type": [
            "string"
          ],
          "enum": [
            "gpt-4o",
            "gpt-3.5-turbo"
          ],
          "enumDescriptions": [
            "Use the (latest) GPT-4o model and works well with projects of all sizes.",
            "Use the GPT-3.5 Turbo model which is cheaper but only suitable from small projects."
          ],
          "enumItemLabels": [
            "GPT-4o",
            "GPT-3.5 Turbo"
          ],
          "default": "gpt-4o",
          "description": "Model to use for code generation. Could be one of gpt-4o or gpt-3.5-turbo for now."
        }
      }
    },
    "menus": {
      "commandPalette": [
        {
          "command": "codeye.startSession"
        },
        {
          "command": "codeye.startSessionAfresh"
        }
      ]
    },
    "terminal": {
      "profiles": [
        {
          "id": "codeye.terminalProfile",
          "icon": "terminal",
          "title": "Codeye"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "vscode-test"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "crc-32": "^1.2.2",
    "dotenv": "^16.4.5",
    "getos": "^3.2.1",
    "mime-types": "^2.1.35",
    "openai": "^4.47.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.80.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  }
}
